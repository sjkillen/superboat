# HTTP
server {
    server_name *.sjkillen.ca sjkillen.ca;
    listen 80;
    proxy_pass 10.0.0.2:80;
}
# HTTPS
server {
    server_name *.sjkillen.ca sjkillen.ca;
    listen 443;
    proxy_pass 10.0.0.2:443;
}
# Gitea SSH
server {
    server_name *.sjkillen.ca sjkillen.ca;
    listen 2222;
    proxy_pass 10.0.0.2:2222;
}
